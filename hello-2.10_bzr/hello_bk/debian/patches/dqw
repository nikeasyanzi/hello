+#define PACKAGE_BUGREPORT "bug-hello@gnu.org"
+
+/* Define to the full name of this package. */
+#define PACKAGE_NAME "GNU Hello"
+
+/* Define to the full name and version of this package. */
+#define PACKAGE_STRING "GNU Hello 2.10"
+
