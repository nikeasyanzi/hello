+STDBOOL_H = 
+STRIP = 
+UNISTD_H = 
+USE_NLS = yes
+VERSION = 2.2
+XGETTEXT = /bin/xgettext
+XGETTEXT_015 = /bin/xgettext
+abs_builddir = /home/craigyang/workplace/hpe_work/package/hello/hello/tests
+abs_srcdir = /home/craigyang/workplace/hpe_work/package/hello/hello/tests
+abs_top_builddir = /home/craigyang/workplace/hpe_work/package/hello/hello
+abs_top_srcdir = /home/craigyang/workplace/hpe_work/package/hello/hello
+ac_ct_CC = gcc
+am__include = include
+am__leading_dot = .
+am__quote = 
+am__tar = ${AMTAR} chof - "$$tardir"
+am__untar = ${AMTAR} xf -
+bindir = ${exec_prefix}/bin
