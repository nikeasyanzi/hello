+MAKEINFO = ${SHELL} /home/craigyang/workplace/hpe_work/package/hello/hello/build-aux/missing --run makeinfo
+MKDIR_P = /bin/mkdir -p
+MSGFMT = /bin/msgfmt
+MSGFMT_015 = /bin/msgfmt
+MSGMERGE = /bin/msgmerge
+OBJEXT = o
+PACKAGE = hello
+PACKAGE_BUGREPORT = bug-hello@gnu.org
+PACKAGE_NAME = GNU Hello
+PACKAGE_STRING = GNU Hello 2.2
+PACKAGE_TARNAME = hello
+PACKAGE_VERSION = 2.2
+PATH_SEPARATOR = :
+POSUB = po
+RANLIB = ranlib
+SET_MAKE = 
