+gl_LIBOBJS =  close-stream.o closeout.o exitfail.o quotearg.o xmalloc.o
+gl_LTLIBOBJS =  close-stream.lo closeout.lo exitfail.lo quotearg.lo xmalloc.lo
+host = x86_64-unknown-linux-gnu
+host_alias = 
+host_cpu = x86_64
