+AMTAR = ${SHELL} /home/craigyang/workplace/hpe_work/package/hello/hello/build-aux/missing --run tar
+AUTOCONF = ${SHELL} /home/craigyang/workplace/hpe_work/package/hello/hello/build-aux/missing --run autoconf
+AUTOHEADER = ${SHELL} /home/craigyang/workplace/hpe_work/package/hello/hello/build-aux/missing --run autoheader
+AUTOMAKE = ${SHELL} /home/craigyang/workplace/hpe_work/package/hello/hello/build-aux/missing --run automake-1.10
+AWK = gawk
+CC = gcc
